{
  "extends": "../../../tsconfig.options.json",
  "include": [
    "../../typescript/tailwind/**/*",
    "../color-scheme/**/*",
    "../components-core/**/*",
    "../components-extra/**/*",
    "../composables/**/*",
    "../i18n/composables/**/*",
    "**/*",
    "**/i18n/**/*.json"
  ],
  "references": [
    {
      "path": "../../typescript/tailwind"
    },
    {
      "path": "../color-scheme"
    },
    {
      "path": "../components-core"
    },
    {
      "path": "../components-extra"
    },
    {
      "path": "../composables"
    },
    {
      "path": "../i18n/composables"
    }
  ],
  "compilerOptions": {
    "outDir": "../../../.moon/cache/types/libs/vue/components-app",
    "paths": {
      "@lychen/typescript-tailwind/*": ["../../typescript/tailwind/*"],
      "@lychen/vue-i18n-composables/*": ["../i18n/composables/*"],
      "@lychen/vue-components-core/*": ["../components-core/*"],
      "@lychen/vue-components-extra/*": ["../components-extra/*"],
      "@lychen/vue-composables/*": ["../composables/*"],
      "@lychen/vue-color-scheme/*": ["../color-scheme/*"]
    }
  }
}
