{
  "extends": "../../../tsconfig.options.json",
  "include": [
    "../../applications/ui-components/**/*",
    "../../typescript/constants/**/*",
    "../i18n/composables/**/*",
    "../ui-components-core/**/*",
    "../ui-components-extra/**/*",
    "../ui-components-website/**/*",
    "**/*"
  ],
  "references": [
    {
      "path": "../../applications/ui-components"
    },
    {
      "path": "../../typescript/constants"
    },
    {
      "path": "../i18n/composables"
    },
    {
      "path": "../ui-components-core"
    },
    {
      "path": "../ui-components-extra"
    },
    {
      "path": "../ui-components-website"
    }
  ],
  "compilerOptions": {
    "outDir": "../../../.moon/cache/types/libs/vue/ui-layouts",
    "paths": {
      "@lychen/vue-ui-components-core/*": ["../ui-components-core/*"],
      "@lychen/applications-ui-components/*": ["../../applications/ui-components/*"],
      "@lychen/typescript-constants/*": ["../../typescript/constants/*"],
      "@lychen/vue-i18n-composables/*": ["../i18n/composables/*"],
      "@lychen/vue-ui-components-extra/*": ["../ui-components-extra/*"],
      "@lychen/vue-ui-components-website/*": ["../ui-components-website/*"]
    }
  }
}
