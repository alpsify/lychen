{
  "extends": "../../tsconfig.options.json",
  "include": [
    "**/*",
    "../../libs/ui-components/**/*",
    "../../libs/ui-css/**/*",
    "../../libs/ui-i18n/**/*",
    "../../libs/ui-layouts/**/*",
    "../../libs/vue/util-composables/**/*",
    "../../libs/vue/util-i18n/**/*",
    "../../libs/vue/util-plausible/**/*"
  ],
  "references": [
    {
      "path": "../../libs/ui-components"
    },
    {
      "path": "../../libs/ui-css"
    },
    {
      "path": "../../libs/ui-i18n"
    },
    {
      "path": "../../libs/ui-layouts"
    },
    {
      "path": "../../libs/vue/util-composables"
    },
    {
      "path": "../../libs/vue/util-i18n"
    },
    {
      "path": "../../libs/vue/util-plausible"
    }
