      "@lychen/ui-css/*": ["../../libs/ui-css/*"],
      "@lychen/ui-i18n/*": ["../../libs/ui-i18n/*"],
      "@lychen/ui-layouts/*": ["../../libs/ui-layouts/*"],
      "@lychen/vue-util-i18n/*": ["../../libs/vue/util-i18n/*"],
      "@lychen/vue-util-composables/*": ["../../libs/vue/util-composables/*"],
      "@lychen/vue-util-plausible/*": ["../../libs/vue/util-plausible/*"]
    },
    "outDir": "../../.moon/cache/types/projects/website"
  }
