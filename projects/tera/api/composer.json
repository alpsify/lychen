{
  "type": "project",
  "license": "proprietary",
  "name": "alpsify/lychen-tera-api",
  "description": "",
  "require": {
    "php": ">=8.3",
    "ext-amqp": "*",
    "ext-ctype": "*",
    "ext-dom": "*",
    "ext-exif": "*",
    "ext-libxml": "*",
    "ext-openssl": "*",
    "ext-xml": "*",
    "api-platform/core": "4.0.*",
    "aws/aws-sdk-php": "3.330.*",
    "doctrine/annotations": "2.0.*",
    "doctrine/cache": "2.2.*",
    "doctrine/doctrine-bundle": "2.13.*",
    "doctrine/doctrine-migrations-bundle": "3.3.*",
    "doctrine/orm": "3.3.*",
    "firebase/php-jwt": "6.10.*",
    "gedmo/doctrine-extensions": "3.17.*",
    "getbrevo/brevo-php": "2.0.*",
    "league/flysystem": "3.29.*",
    "league/flysystem-aws-s3-v3": "3.29.*",
    "nelmio/cors-bundle": "2.5.*",
    "oneup/flysystem-bundle": "4.12.*",
    "phpdocumentor/reflection-docblock": "5.6.*",
    "predis/predis": "2.3.*",
    "runtime/frankenphp-symfony": "0.2.*",
    "scienta/doctrine-json-functions": "6.3.*",
    "sentry/sentry-symfony": "5.1.*",
    "snc/redis-bundle": "4.7.*",
    "stripe/stripe-php": "16.3.*",
    "symfony/amqp-messenger": "7.2.*",
    "symfony/asset": "7.2.*",
    "symfony/brevo-mailer": "7.2.*",
    "symfony/clock": "7.2.*",
    "symfony/config": "7.2.*",
    "symfony/console": "7.2.*",
    "symfony/doctrine-messenger": "7.2.*",
    "symfony/dotenv": "7.2.*",
    "symfony/expression-language": "7.2.*",
    "symfony/flex": "2.4.*",
    "symfony/form": "7.2.*",
    "symfony/framework-bundle": "7.2.*",
    "symfony/http-client": "7.2.*",
    "symfony/intl": "7.2.*",
    "symfony/mailer": "7.2.*",
    "symfony/messenger": "7.2.*",
    "symfony/monolog-bundle": "3.10.*",
    "symfony/process": "7.2.*",
    "symfony/property-access": "7.2.*",
    "symfony/property-info": "7.2.*",
    "symfony/rate-limiter": "7.2.*",
    "symfony/runtime": "7.2.*",
    "symfony/security-bundle": "7.2.*",
    "symfony/serializer": "7.2.*",
    "symfony/translation": "7.2.*",
    "symfony/twig-bundle": "7.2.*",
    "symfony/uid": "7.2.*",
    "symfony/validator": "7.2.*",
    "symfony/workflow": "7.2.*",
    "symfony/yaml": "7.2.*"
  },
  "require-dev": {
    "dama/doctrine-test-bundle": "8.2.*",
    "doctrine/doctrine-fixtures-bundle": "3.7.*",
    "justinrainbow/json-schema": "6.0.*",
    "mtdowling/jmespath.php": "2.8.*",
    "phpstan/phpstan-doctrine": "^1.5",
    "phpunit/phpunit": "11.5.*",
    "symfony/browser-kit": "7.2.*",
    "symfony/css-selector": "7.2.*",
    "symfony/debug-bundle": "7.2.*",
    "symfony/maker-bundle": "1.61.*",
    "symfony/phpunit-bridge": "7.2.*",
    "symfony/stopwatch": "7.2.*",
    "symfony/web-profiler-bundle": "7.2.*",
    "zenstruck/foundry": "2.2.*",
    "zenstruck/messenger-test": "1.11.*"
  },
  "repositories": [],
  "config": {
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true,
    "allow-plugins": {
      "symfony/flex": true,
      "symfony/runtime": true
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "paragonie/random_compat": "2.*",
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php71": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php56": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "doctrine:cache:clear-metadata": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": true,
      "require": "7.2.*",
      "docker": true
    }
  }
}
