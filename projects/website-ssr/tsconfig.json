{
  "extends": "./.nuxt/tsconfig.json",
  "references": [
    {
      "path": "../../libs/ui-components"
    },
    {
      "path": "../../libs/ui-css"
    },
    {
      "path": "../../libs/ui-i18n"
    },
    {
      "path": "../../libs/ui-layouts"
    },
    {
      "path": "../../libs/vue/util-composables"
    },
    {
      "path": "../../libs/vue/util-i18n"
    }
  ],
  "include": [
    "../../libs/ui-components/**/*",
    "../../libs/ui-css/**/*",
    "../../libs/ui-i18n/**/*",
    "../../libs/ui-layouts/**/*",
    "../../libs/vue/util-composables/**/*",
    "../../libs/vue/util-i18n/**/*"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "outDir": "../../.moon/cache/types/projects/website-ssr",
    "paths": {
      "@pages/*": ["pages/*"],
      "@composables/*": ["composables/*"],
      "@lychen/ui-components/*": ["../../libs/ui-components/*"],
      "@lychen/ui-css/*": ["../../libs/ui-css/*"],
      "@lychen/ui-i18n/*": ["../../libs/ui-i18n/*"],
      "@lychen/ui-layouts/*": ["../../libs/ui-layouts/*"],
      "@lychen/vue-util-composables/*": ["../../libs/vue/util-composables/*"],
      "@lychen/vue-util-i18n/*": ["../../libs/vue/util-i18n/*"]
    }
  }
}
