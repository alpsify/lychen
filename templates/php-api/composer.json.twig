{
  "type": "project",
  "license": "proprietary",
  "name": "lychen/{{ name }}-api",
  "description": "",
  "require": {
    "php": ">=8.4",
    "ext-amqp": "*",
    "ext-ctype": "*",
    "ext-dom": "*",
    "ext-exif": "*",
    "ext-libxml": "*",
    "ext-openssl": "*",
    "ext-xml": "*",
    "api-platform/core": "4.1.*",
    "doctrine/annotations": "2.0.*",
    "doctrine/cache": "2.2.*",
    "doctrine/doctrine-bundle": "2.14.*",
    "doctrine/doctrine-migrations-bundle": "3.4.*",
    "doctrine/orm": "3.5.*",
    "gedmo/doctrine-extensions": "3.20.*",
    "lychen/core": "*",
    "lychen/model": "*",
    "lychen/tiptap": "*",
    "lychen/zitadel": "*",
    "martin-georgiev/postgresql-for-doctrine": "^3.3",
    "nelmio/cors-bundle": "2.5.*",
    "phpdocumentor/reflection-docblock": "5.6.*",
    "predis/predis": "3.1.*",
    "runtime/frankenphp-symfony": "0.2.*",
    "scienta/doctrine-json-functions": "*",
    "sentry/sentry-symfony": "5.3.*",
    "snc/redis-bundle": "4.10.*",
    "symfony/amqp-messenger": "7.3.*",
    "symfony/asset": "7.3.*",
    "symfony/clock": "7.3.*",
    "symfony/config": "7.3.*",
    "symfony/console": "7.3.*",
    "symfony/doctrine-messenger": "7.3.*",
    "symfony/dotenv": "7.3.*",
    "symfony/expression-language": "7.3.*",
    "symfony/flex": "2.4.*",
    "symfony/form": "7.3.*",
    "symfony/framework-bundle": "7.3.*",
    "symfony/http-client": "7.3.*",
    "symfony/intl": "7.3.*",
    "symfony/mailer": "7.3.*",
    "symfony/messenger": "7.3.*",
    "symfony/monolog-bundle": "3.10.*",
    "symfony/process": "7.3.*",
    "symfony/property-access": "7.3.*",
    "symfony/property-info": "7.3.*",
    "symfony/rate-limiter": "7.3.*",
    "symfony/runtime": "7.3.*",
    "symfony/security-bundle": "7.3.*",
    "symfony/serializer": "7.3.*",
    "symfony/translation": "7.3.*",
    "symfony/twig-bundle": "7.3.*",
    "symfony/uid": "7.3.*",
    "symfony/validator": "7.3.*",
    "symfony/workflow": "7.3.*",
    "symfony/yaml": "7.3.*"
  },
  "require-dev": {
    "dama/doctrine-test-bundle": "8.3.*",
    "doctrine/doctrine-fixtures-bundle": "4.1.*",
    "justinrainbow/json-schema": "6.4.*",
    "mtdowling/jmespath.php": "2.8.*",
    "phpstan/phpstan-doctrine": "^1.5",
    "phpunit/phpunit": "12.2.*",
    "symfony/browser-kit": "7.3.*",
    "symfony/css-selector": "7.3.*",
    "symfony/debug-bundle": "7.3.*",
    "symfony/maker-bundle": "1.64.*",
    "symfony/phpunit-bridge": "7.3.*",
    "symfony/stopwatch": "7.3.*",
    "symfony/web-profiler-bundle": "7.3.*",
    "zenstruck/browser": "^1.9",
    "zenstruck/foundry": "2.6.*",
    "zenstruck/messenger-test": "1.11.*"
  },
  "repositories": [
    {
      "type": "path",
      "url": "../packages/*",
      "options": {
        "symlink": false,
        "versions": {
          "lychen/model": "0.0.0",
          "lychen/zitadel": "0.0.0",
          "lychen/tiptap": "0.0.0",
          "lychen/core": "0.0.0"
        }
      }
    }
  ],
  "config": {
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true,
    "allow-plugins": {
      "symfony/flex": true,
      "symfony/runtime": true
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "paragonie/random_compat": "2.*",
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php71": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php56": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "doctrine:cache:clear-metadata": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": true,
      "require": "7.3.*",
      "docker": true
    }
  }
}
